import java.util.*;

class rough{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);

        System.out.print("enter the number : ");
        int n = sc.nextInt();
        int arr[] = new int[8];
        int z=0;

        while(n != 0){
            int temp = n%2;
            arr[z] = temp;
            n /= 2;
            z++;
        }
        for(int i=7; i>=0; i--){
            System.out.print(arr[i]);
        }
    }
}
